#CLASS datetime READ {./pkg/pkg_datetime.tin};

#CLASS room OPEN;

#ACTION {^%u - $} {
    #VARIABLE env[room] {%1};
} {1};

#ACTION {    这里%u的出口是%*。} {
    #VARIABLE {env[exits]} {%2};
    #VARIABLE {env[room_updated]} {@GetEpoch{}};
} {1};

#ACTION {    这里没有明显的出口。} {
    #VARIABLE {env[exits]} {};
    #VARIABLE {env[room_updated]} {@GetEpoch{}};
}

#CLASS room CLOSE;