#NOP graph tests.;

#CLASS testing READ {./pkg/pkg_testing.tin};
#CLASS graph READ {./pkg/pkg_graph.tin};
#CLASS login READ {./pkg/pkg_login.tin};

#VARIABLE {username} {testgraph};
#VARIABLE {password} {Abcd_124};

#ALIAS test_2 {
    #VARIABLE {test[name]} {欢迎来到天龙八部-洛阳中心};
    #VARIABLE {test[expected_room]} {洛阳中心 south、north、east、west 和 up};
    #VARIABLE {result} {@_dijkstra{欢迎来到天龙八部 south 和 down;洛阳中心 south、north、east、west 和 up}};
    #ECHO {Result: $result};
};

#ALIAS test_3 {
    #VARIABLE {test[name]} {欢迎来到天龙八部-黄土大道};
    #VARIABLE {test[expected_room]} {黄土大道 southeast 和 west};
    
    #VARIABLE {result} {@_dijkstra{欢迎来到天龙八部 south 和 down;黄土大道 southeast 和 west}};
    #ECHO {Result: $result};
};

#ALIAS Dijkstra_Test {
    #LOCAL src {%1};
    #LOCAL dest {%2};

    #VARIABLE {test[name]} {$src-$dest};
    #VARIABLE {test[expected_room]} {$dest};
    #VARIABLE {res} {@_dijkstra{$src;$dest}};
    #ECHO {Result: $res};
}

#ALIAS Travel_Test {
    #LOCAL src {%1};
    #LOCAL dest {%2};

    #VARIABLE {test[name]} {$src-$dest};
    #VARIABLE {test[expected_room]} {$dest};

    start_test;
    travel {$src} {$dest};
};

#NOP Dijkstra_Test {欢迎来到天龙八部 south 和 down} {汴梁城中心 north、south、east、west 和 up};
#NOP Dijkstra_Test {欢迎来到天龙八部 south 和 down} {洛阳中心 south、north、east、west 和 up};

#EVENT {VARIABLE UPDATED env[last_login]} {
    Travel_Test {欢迎来到天龙八部 south 和 down} {大道 north、south 和 northeast};
};

conn;



