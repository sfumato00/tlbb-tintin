#NOP 

#CLASS tlbb read {./tlbb.tin};

#ALIAS {conn} {
    #VARIABLE {env} {
        {profile} {./profile_%1.tin}
    };
    #CLASS $profile read {$env[profile]};
    #SESSION {$username} {103.10.85.133} {8888};
};

#EVENT {SESSION DISCONNECTED} {
    #gts #DELAY {3} {
        conn $profile;
    };
}

#EVENT {SESSION TIMED OUT} {
    #gts #DELAY {3} {
        conn $profile;
    };
};